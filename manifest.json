{
  "manifest_version": 3,
  "name": "Correlation Calculator",
  "version": "1.0",
  "description": "Calculates correlation percentage between two texts",
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Click Me",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
