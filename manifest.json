{
  "manifest_version": 2,
  "name": "Correlation Calculator",
  "version": "1.0",
  "description": "Calculates correlation percentage between two texts",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "permissions": ["activeTab"]
}
