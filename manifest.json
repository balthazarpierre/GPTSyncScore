{
  "manifest_version": 3,
  "name": "GPTSyncScore",
  "version": "1.0",
  "description": "Compute the GPTSyncScore : correlation percentage between orginal text and final text generated by chatGPT",
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "GPTSyncScore",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
